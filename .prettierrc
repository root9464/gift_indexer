{
  "plugins": ["prettier-plugin-tailwindcss"],
  "jsxSingleQuote": true,
  "singleQuote": true,
  "semi": true,
  "printWidth": 145,
  "tabWidth": 2,
  "arrowParens": "always",
  "proseWrap": "always",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "singleAttributePerLine": false,
  "bracketSameLine": true,
  "htmlWhitespaceSensitivity": "ignore"
}
